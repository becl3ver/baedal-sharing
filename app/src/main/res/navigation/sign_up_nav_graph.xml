<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sign_up_nav_graph"
    app:startDestination="@id/termsOfServiceFragment">
    <fragment
        android:id="@+id/termsOfServiceFragment"
        android:name="com.fourfifths.android.baedalsharing.view.auth.TermsOfServiceFragment"
        android:label="TermsOfServiceFragment" >
        <action
            android:id="@+id/action_termsOfServiceFragment_to_completeSignUpFragment"
            app:destination="@id/completeSignUpFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/completeSignUpFragment"
        android:name="com.fourfifths.android.baedalsharing.view.auth.CompleteSignUpFragment"
        android:label="CompleteSignUpFragment" />
</navigation>